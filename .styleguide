cppHeaderFileInclude {
  \.h$
  \.hpp$
  \.inl$
  \.inc$
}

cppSrcFileInclude {
  \.cpp$
}

modifiableFileExclude {
  gradle/
  python/
  format.py
  make.py
}

includeCppSys {
  experimental/
}

includeOtherLibs {
  frc/
  gtest/
  hal/
  mockdata/
  networktables/
  rev/
  wpi/
  cscore\.h$
  units\.h$
}

generatedFileExclude {
  thirdparty/cpp/drake/
  thirdparty/include/Eigen/
  thirdparty/include/drake/
  thirdparty/include/units/
  thirdparty/include/unsupported/
  thirdparty/include/wpi/
  src/test/cpp/drake/
  src/test/include/drake/
}
